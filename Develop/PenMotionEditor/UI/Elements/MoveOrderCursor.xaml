             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PenMotionEditor.UI.Windows"
             mc:Ignorable="d">
    <Grid Height="40">
		<Rectangle Fill="#EACB9E" Stroke="#B5B5B5"></Rectangle>
		<TextBlock Name="NameTextBlock" Text="SelectedItemName" FontFamily="/SweepMotionEditor;component/Resource/Font/#KoPubDotum_Pro Bold" FontSize="14" TextOptions.TextHintingMode="Animated" VerticalAlignment="Center" Foreground="#FF343434" Margin="8,0,0,0" HorizontalAlignment="Left"></TextBlock>
